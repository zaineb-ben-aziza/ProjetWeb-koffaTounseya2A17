<script type="text/javascript" language="javascript">
    $(document).ready(function () {
        $("[id*=btnLikeComment]").click(function () {
            var commentId = this.id.split('_');
            likecomment(commentId[1])
        });
        $("#commentForm").submit(function () {
            $('span[id*= "msgValidComment"]').attr("style", "visibility: hidden");
            $('span[id*= "captchaTextRequired"]').attr("style", "visibility: hidden");
            $('span[id*= "captchaTextValid"]').attr("style", "visibility: hidden");
            var validData = true;
            //Validation de données : tous les champs sont obligatoires.
            if ($('[name="txtCaptcha"]').val() == "") {
                $('span[id*= "captchaTextRequired"]').attr("style", "visibility: visible");
                validData = false;
            }
            else {
                var methodParams = { mdText: $('[name="hdnCaptcha"]').val(), msgText: $('[name="txtCaptcha"]').val() };
                var stringMethodParams = JSON.stringify(methodParams);
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: 'http://www.chahiatayba.com/Controls/Captcha/SecurityHelper.asmx/IsValidTextMD5',
                    data: stringMethodParams,
                    dataType: "json",
                    async: false,
                    success: function (data, text) {
                        if (data != null) {
                            if (!data.d) {
                                validData = false;
                                $('span[id*= "captchaTextValid"]').attr("style", "visibility: visible");
                            }

                        }
                    },
                    error: function (request, status, error) {
                        alert('problème lors de l\'appel au service de vérification captcha');

                    }
                });


            }

            if ($('#commentText').val() == "" || $('#commentName').val() == "" || $('#commentEmail').val() == "") {
                $('span[id*= "msgValidComment"]').attr("style", "visibility: visible");
                validData = false;
            }

            return validData;

        });

        

    });

    function likecomment(commentId) {
        var methodParams = { commentId: commentId, category: '2' };
        var stringMethodParams = JSON.stringify(methodParams);
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: 'http://www.chahiatayba.com/Services/CommentService.asmx/likeComment',
            data: stringMethodParams,
            dataType: "json",
            async: false,
            success: function (data, text) {
                if (data != null) {
                    //like comment OK 
                    if (data.d > 0) {
                        $('#nblike_' + commentId).html(data.d);
                        
                    }
                    //already like comment
                    else if (data.d == 0) {
                        var opts = {
                            title: 'Notification',
                            text: "Vous aimez déjà ce commentaire !",
                            addclass: "stack-bottomright",
                            //addclass: "stack-custom2",
                            pnotify_delay: 3000,
                            pnotify_stack: stack_bottomright,
                            pnotify_type: "notice"
                        };

                        $.pnotify(opts);
                    }
                    //non connecté
                    else if (data.d == -1) {
                        var identificationUrl = 'http://www.chahiatayba.com/login.aspx?returnurl=%2frecettes%2f233%2fkefta-tunisienne?likecomment=' + commentId;
                        var opts = {
                            title: 'Notification',
                            text: 'Veuillez vous <a href="' + identificationUrl + '">connecter</a>',
                            addclass: "stack-bottomright",
                            //addclass: "stack-custom2",
                            pnotify_delay: 3000,
                            pnotify_stack: stack_bottomright,
                            pnotify_type: "notice"
                        };

                        $.pnotify(opts);
                    }
                    //erreur serveur
                    else {

                    }

                }
            },
            error: function (request, status, error) {
                alert('problème lors de l\'appel au service de vérification captcha');

            }
        });
    }
    
</script>

